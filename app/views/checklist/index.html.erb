<h1>Make a Checklist</h1>
<fieldset><legend>Checklist</legend>
<form id="list">
  <input type="text" />
  <button>Add Checklist Item</button>
</form>
</fieldset>

<ul>
  <% @allinfo.each do |item| %>
  <% if item.is_checked == true %>
  <li class="strike"><%= item.tasks %>
  <input type="checkbox", checked="checked", id="<%= item.id %>"></li>
  <% else %>
  <li><%= item.tasks %>
  <input type="checkbox", id="<%= item.id %>"></li>
  <% end %>
  <% end %>
</ul>

